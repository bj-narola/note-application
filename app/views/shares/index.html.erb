<br>
<div class="col-lg-12">
  <div class="row">
    <div class="col-lg-12">
      <!-- Nav tabs -->
      <ul class="nav nav-pills" role="tablist">
        <li class="nav-item">
          <%= link_to "Notes Shared To Me", shared_notes_with_me_shares_path, class: "nav-link active", remote: true, id: "shared_to_me" %>
        </li>
        <li class="nav-item">
          <%= link_to "Notes Shared By Me", shared_notes_by_me_shares_path, class: "nav-link", remote: true, id: "shared_by_me" %>
        </li>
      </ul>

      <div class="row">
        <div class="col-lg-12">
          <div class="row">
            <div class="col-lg-3">
              <br>
              <center><h4></h4><center>
            </div>
            <div class="col-lg-9">
              <h4 class="modal-title1"></h4>
            </div>
          </div>
          <div class="row">
            <div class="col-lg-3" style="border: 0px solid;height:550px; overflow: scroll;">
              <table class="table table-bordered table-striped" style="padding: 25px; width:90% !important" align="center">
                <tbody id="notes">
                <%= render 'data' %>
                </tbody>
              </table>
            </div>
            <div class="col-lg-9" style="border: 0px solid; padding-right: 20px" id="note_comment">
              <%= render 'info' %>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<!-- all modals -->
<div class="modal fade" id="comment"></div>
<div class="modal fade" id="invitation_email"></div>
<%= javascript_include_tag 'note', 'data-turbolinks-track': 'reload' %>
